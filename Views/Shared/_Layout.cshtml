        function GetAllMasterClients() {
            $.ajax({
                type: 'GET',
                url: '/Administration/GetAllClients',
                async: false,
                dataType: 'json',
                contentType: "application/json",
                cache: false,
                callback: true,
                crossDomain: true,
                success: function (data) {
                    $('#ddlMasterClient').empty();
                    var markup = "<option value=''>- Select Client -</option>";
                    if (data != null && data != "Content not found") {
                        $.each(data, function (i, item) {
                            markup += "<option value=" + item.DatabaseId + ">" + item.ClientName + "</option>";
                        });
                    }
                    @*else {
                    if ('@TempData["errorMessage"]' != null && '@TempData["errorMessage"]' != "") {
                        NotifyMessage('@TempData["errorMessage"]', 'top-right', '1800', 'warning', 'fa-warning', true);
                    }
                }*@
                    $('#ddlMasterClient').html(markup).show();
                }
            });
        }

        function SessionClientID() {
            var DatabaseId = $("#ddlMasterClient :selected").val();
            var ClientName = $("#ddlMasterClient :selected").text();
            $.ajax({
                type: 'POST',
                async: true,
                url: '/Login/ChangeClientDetails',
                data: { DatabaseId: DatabaseId, ClientName: ClientName },
                success: function (data) {
                }
            });
        }

        function NotifyMessage(message, position, timeout, theme, icon, closable) {
            toastr.options.positionClass = 'toast-' + position;
            toastr.options.extendedTimeOut = 0; //1000;
            toastr.options.timeOut = timeout;
            toastr.options.closeButton = closable;
            toastr.options.iconClass = icon + ' toast-' + theme;
            toastr['custom'](message);
        }


</script>

