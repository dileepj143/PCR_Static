    <script>
        function ActiveSideBar() {

            var urlPath = window.location.pathname;
            urlPath = urlPath.split('/');
            if (urlPath[1] == "Onboarding" && urlPath[2] != "ManagePreOnboarding") {
                $('#sidebar #All li').hide();
                $('#sidebar #All #seventh').show();
                $('#sidebar #All #seventh ul li').show();
            }
            else {
                $('#sidebar #All li').show();
                $('#sidebar #All #seventh').hide();
            }

            if ('@ViewBag.RoleID' == 4) {
                if (urlPath[1] == "UserManagement" && urlPath[2] == "Profile") {
                    $('#sidebar #All li').hide();
                    $('#sidebar #All #seventh').show();
                    $('#sidebar #All #seventh ul li').show();
                }
            }

            $('#sidebar #All li').removeClass('active');
            $("#sidebar #All a").each(function (e) {
                var url = $(this).attr("href");
                var urlId = $(this).attr("id");

                if (url == window.location.pathname) {
                    $(this).parent().addClass('active');
                    $(this).parent().parent().parent().addClass('open');
                    return false;
                }
                else if ((window.location.pathname).indexOf(urlId) != -1) {
                    $(this).parent().addClass('active');
                    $(this).parent().parent().parent().addClass('open');
                    return false;
                }
            });
        }
        </script>
