@{
    ViewData["Title"] = "Super Admin";
}
 <!-- Page Breadcrumb -->
<div class="page-breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="fa fa-home themeprimary"></i>
            @if (ViewBag.RoleID == 1)
            {
                <a href="/SuperAdmin/Index">Dashboard</a>
            }
            else if (ViewBag.RoleID == 2)
            {
                <a href="/ClientAdmin/Index">Dashboard</a>
            }
            else if (ViewBag.RoleID == 3)
            {
                <a href="/Manager/Index">Dashboard</a>
            }
            else
            {
                <a href="/Employee/I9categories">Dashboard</a>
            }
        </li>

        <li class="active">I9 Section-3</li>
    </ul>
</div>
<!-- /Page Breadcrumb -->
<!-- Page Header -->
<div class="page-header position-relative">
    <div class="header-title">
        <h1 class="themeprimary">
            I9 Section-3: Reverification and Rehires
        </h1>
    </div>
    <!--Header Buttons-->
    <div class="header-buttons">
        <a class="sidebar-toggler" href="#">
            <i class="fa fa-arrows-h"></i>
        </a>
        <a class="refresh" id="refresh-toggler" href="">
            <i class="glyphicon glyphicon-refresh"></i>
        </a>
        <a class="fullscreen" id="fullscreen-toggler" href="#">
            <i class="glyphicon glyphicon-fullscreen"></i>
        </a>
    </div>
    <!--Header Buttons End-->
</div>
<!-- /Page Header -->
<!-- Page Body -->
<div class="page-body">
    <div class="col-lg-12 col-sm-12 col-xs-12" style="padding:0px;">
        <!-- <h5 class="row-title before-maincolor widget-caption">Add New Division</h5> -->
        <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12 division-form">
            <form class="form-horizontal" role="form">
                <div class="form-group border-formgroup col-md-12 col-lg-12 col-sm-12 col-xs-12">
                    <span class="starthere-h4"><strong>Section 3. Reverification and Rehires</strong></span> <span class="starthere-p">(To be completed and signed by employer or authorized representative.)</span>
                </div>
                <div class="form-group border-formgroup-right col-md-7 col-lg-7 col-sm-7 col-xs-12">
                    <div class="col-md-4 col-lg-4 no-padding-left">
                        <label for="inputEmail3" class="control-label">A. New Name (if applicable)</label>
                    </div>
                    <div class="col-md-8 col-lg-8">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="Don">
                    </div>
                </div>
                <div class="form-group border-formgroup col-md-5 col-lg-5 col-sm-5 col-xs-12">
                    <div class="col-md-6 col-lg-6 no-padding-left">
                        <label for="inputEmail3" class="control-label">B. Date of Rehire (if applicable)</label>
                    </div>
                    <div class="col-md-6 col-lg-6">
                        <input type="text" class="form-control" id="inputEmail3" placeholder="12/5/2016">
                    </div>
                </div>
                <div class="form-group border-formgroup-right col-md-4 col-lg-4 col-sm-4 col-xs-12">
                    <label for="inputEmail3" class="control-label">Last Name (Family Name)</label>
                    <input type="text" class="form-control" id="inputEmail3" placeholder="john">
                </div>
                <div class="form-group border-formgroup-right col-md-3 col-lg-3 col-sm-3 col-xs-12">
                    <label for="inputEmail3" class="control-label">First Name (Given Name)</label>
                    <input type="text" class="form-control" id="inputEmail3" placeholder="lewis">
                </div>
                <div class="form-group border-formgroup-right col-md-2 col-lg-2 col-sm-2 col-xs-12">
                    <label for="inputEmail3" class="control-label">Middle Initial</label>
                    <input type="text" class="form-control" id="inputEmail3" placeholder="mark">
                </div>
                <div class="form-group border-formgroup col-md-3 col-lg-3 col-sm-3 col-xs-12">
                    <label for="inputEmail3" class="control-label">Date (mm/dd/yyyy)</label>
                    <input type="text" class="form-control" id="inputEmail3" placeholder="5/12/207">
                </div>
            </form>
            <hr class="i9section3-hr">
            <div class="col-md-6 col-md-offset-5 col-lg-offset-5 col-lg-6 col-sm-12 col-xs-12 form-btns">
                <!-- <a href="#" class="cancel-btn"><button  type="button" class="btn btn-md btn-primary">Cancel</button></a> -->
                <a href="/SuperAdmin/Signin" class="cancel-btn"><button type="button" class="btn btn-md btn-primary">Previous</button></a>
                <a href="/SuperAdmin/Managerlogin" class="cancel-btn"><button type="button" class="btn btn-md btn-primary">Next</button></a>
            </div>
        </div>
    </div>
    <!-- /Page Content -->
</div>