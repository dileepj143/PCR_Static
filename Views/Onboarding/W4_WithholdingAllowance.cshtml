        /*13-05 CSS */

        .form-w-4-personal table tr td {
            vertical-align: top;
            line-height: 22px;
            border-top: none;
            text-align: justify;
        }

        .security-card:before {
            float: right;
        }

        ul {
            padding-left: 16px;
        }

        .table > tbody > tr > td {
            padding-left: 0px;
        }
    </style>
