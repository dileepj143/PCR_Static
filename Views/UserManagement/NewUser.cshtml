    <environment>
        <!--Page Related Scripts-->
        <script src="~/assets/js/toastr/toastr.js"></script>
        <script src="~/assets/js/inputmask/jasny-bootstrap.min.js"></script>
        <script src="~/assets/js/validation/bootstrapValidator.js"></script>
        <script src="~/assets/js/bootbox/bootbox.js"></script>
        <script>
        var pcrUsers = "";
        $(document).ready(function ()
        {
            if ('@TempData["Message"]' != null && '@TempData["Message"]' != "")
            {
                NotifyMessage('@TempData["Message"]', 'top-right', '1800', 'warning', 'fa-warning', true);
            }

            $("#ddlMasterClient").prop("disabled", true).css("cursor", "default").fadeTo(500, "none");

            Reporting();

            $('#registrationForm .name').keypress(function (key) {
                if ((key.charCode < 97 || key.charCode > 122) && (key.charCode < 65 || key.charCode > 90) && (key.charCode != 45) && (key.charCode != 32)) return false;
            });

            if ('@ViewBag.RoleID' == 1) {$('#RoleID option[value=1]').remove();}
            else if ('@ViewBag.RoleID' == 2) {$('#RoleID option[value=1]').remove();$('#RoleID option[value=2]').remove();}
            else if ('@ViewBag.RoleID' == 3) { $('#RoleID option[value=1]').remove(); $('#RoleID option[value=2]').remove(); $('#RoleID option[value=3]').remove(); }

            //ZipCode Validation
            $("#txtZipCode").keypress(function (e) {
                if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {return false;}
                if ($("#txtZipCode").val().length > 4) {return false;}
            });
            // WorkPhone Number Validation to accept numbers only
            $("#txtwrkphone").keypress(function (e) {
                if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) { return false; }
                if ($("#txtwrkphone").val().length > 10) {return false;}
            });
            // Mobile Number Validation
            $("#txtMobile").keypress(function (e) {
                if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {return false;}
                if ($("#txtMobile").val().length > 9) {return false; }
            });

            $('#txtLastName, #txtFirstName, #CityID').keydown(function (e) {
                if (e.ctrlKey || e.altKey)
                {
                    e.preventDefault();
                }
                else
                {
                    var key = e.keyCode;
                    if (!((key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90) || (key == 9))) {
                        e.preventDefault();
                    }
                }
            });


            $('input:checkbox[name="IsPCR"]').on('change', function () {
                if ($(this).is(":checked"))
                {
                    $('#ddlPCRUserDiv').show();
                    $('#ddlPCRUsersLinkingDiv').show();
                    //$('#ddlPCRUsers option[value=""]').prop('selected', true);
                    GetPCRUsers();    // To get the PCR user list.
                }
                else {
                    $('#ddlPCRUserDiv').hide();
                    $('#ddlPCRUsersLinkingDiv').hide();
                }
            });

            $('#ddlPCRUsers').change(function ()
            {
                $('#lstBox4').empty();
                var pcrUserId = $(this).val();
                $('#lstBox3').html(pcrUsers);
                $('#lstBox3 option[value=""]').remove();
                $('#lstBox3 option[value=' + pcrUserId + ']').remove();
            });


        });

        function Reporting() {
            $.ajax({
                type: 'GET',
                url: '/Manager/GetAllUserNames',
                async: false,
                dataType: 'json',
                contentType: "application/json; charset=utf-8",
                cache: false,
                callback: true,
                crossDomain: true,
                success: function (data)
                {
                    $('#SelectUserID').empty();
                    if (data != null && data != "Content not found") {
                        var markup = "<option value=''>- Select Reporting Authority -</option>";
                        $.each(data, function (i, item) {
                            markup += "<option value=" + item.EmailAddress + ">" + item.LastName + " " + item.FirstName + " - (" + item.EmailAddress + ")</option>";
                        });
                        $('#SelectUserID').html(markup).show();
                    }
                }
            });
        }

        function NotifyMessage(message, position, timeout, theme, icon, closable) {
            toastr.options.positionClass = 'toast-' + position;
            toastr.options.extendedTimeOut = 0; //1000;
            toastr.options.timeOut = timeout;
            toastr.options.closeButton = closable;
            toastr.options.iconClass = icon + ' toast-' + theme;
            toastr['custom'](message);
        }

        function GetPCRUsers() {
            $('#ddlPCRUsers').empty();
            $('#lstBox3').empty();
            pcrUsers = "<option value=''>- Select Pcr User -</option>";
            $.ajax({
                type: 'GET',
                url: '/UserManagement/GetPcrUsers',
                async: false,
                dataType: 'json',
                contentType: "application/json",
                cache: false,
                callback: true,
                crossDomain: true,
                success: function (data)
                {
                    if (data != null && data != "Invalid Session Id")
                    {
                        $.each(data.Results, function (i, item)
                        {
                            pcrUsers += "<option value=" + item.FullName + ">" + item.UserName + "</option>";
                        });
                    }
                }
            });
            $('#ddlPCRUsers').html(pcrUsers).show();
            $('#lstBox3').html(pcrUsers).show();
            $('#lstBox3 option[value=""]').remove();
        }

        </script>

        <script>
	(function () {
  $('#btnRight').click(function (e) {
      var selectedOpts = $('#lstBox1 option:selected');
      if (selectedOpts.length == 0) {
          alert("Nothing to move.");
          e.preventDefault();
      }
      $('#lstBox2').append($(selectedOpts).clone());
      $(selectedOpts).remove();
      e.preventDefault();
  });
  $('#btnLeft').click(function (e) {
      var selectedOpts = $('#lstBox2 option:selected');
      if (selectedOpts.length == 0) {
          alert("Nothing to move.");
          e.preventDefault();
      }
      $('#lstBox1').append($(selectedOpts).clone());
      $(selectedOpts).remove();
      e.preventDefault();
  });
}(jQuery));
        </script>

        <script>
	(function () {
  $('#btnRight2').click(function (e) {
      var selectedOpts = $('#lstBox3 option:selected');
      if (selectedOpts.length == 0) {
          alert("Nothing to move.");
          e.preventDefault();
      }
      $('#lstBox4').append($(selectedOpts).clone());
      $(selectedOpts).remove();
      e.preventDefault();
  });
  $('#btnLeft2').click(function (e) {
      var selectedOpts = $('#lstBox4 option:selected');
      if (selectedOpts.length == 0) {
          alert("Nothing to move.");
          e.preventDefault();
      }
      $('#lstBox3').append($(selectedOpts).clone());
      $(selectedOpts).remove();
      e.preventDefault();
  });
}(jQuery));
        </script>

    </environment>
